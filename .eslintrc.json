{
    "extends": "airbnb",
    "rules": {
        "indent": ["error", 4, {"ignoredNodes": ["JSXElement"]}],
        "react/jsx-indent-props": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "react/jsx-closing-bracket-location": [0, "after-props"],
        "react/jsx-tag-spacing": ["error", {
            "beforeSelfClosing": "always",
            "beforeClosing": "never"
        }]
    }
}